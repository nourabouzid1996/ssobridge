{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "esModuleInterop": true,
    "types": ["react", "react-dom"]
  },
  "exclude": ["dist/"]
}
